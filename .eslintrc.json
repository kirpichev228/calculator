{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings",
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "jsx-quotes": [
            1,
            "prefer-double"
        ],
        "no-console": 1,
        "no-debugger": 2,
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ]
    }
}
